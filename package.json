{
  "name": "vue-ssr-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env RUN_ENV=client vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build:client": "cross-env RUN_ENV=client  vue-cli-service build",
    "build:server": "cross-env RUN_ENV=server  vue-cli-service build",
    "service": "node server.js",
    "start": "npm run build:server && npm run build:client && npm run service"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "koa": "^2.13.0",
    "koa-static": "^5.0.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^1.19.1",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.11"
  }
}
